<%- include('partials/header') %>

    <h3 class="dashboardHead">Welcome to dashboard!!!</h3>
    <hr class="dashhr">



    <div class="dashboardContainer">

        <div class="dashsidepanel">
            <h4>Click to insert data!</h4>

            <ul>
                <li><a class="" href="#shop-div"> Add Shop</a></li>
                <li><a href="#product-div"> Add Products</a></li>

            </ul>
            <h4>Click to view data!</h4>
            <ul>
                <li><a href="#shops"> View Shop</a></li>
                <li><a href="#products"> View Product</a></li>
                <li><a href="#orders"> View Orders</a></li>
                <li><a href="#visits"> View Visits</a></li>
                <li><a href="#contains"> View Contains</a></li>

            </ul>

        </div>



        <div class="insertContainer" id="shop-div">
            <h2 class="insert-name">insert new Shop</h2>

            <form action="/addShop" method="post">

                <ul class="insertlist">
                    <li class="insert-li"> <input placeholder="Shop id" name="id" type="text" class="insert-input"> </li>
                    <li class="insert-li"> <input placeholder="Shop name" name="name" type="text" class="insert-input"> </li>
                    <li class="insert-li"> <input placeholder="Shop loc" name="loc" type="text" class="insert-input"> </li>
                    <li class="insert-li"> <input placeholder="Shop phone" name="phone" type="text" class="insert-input"> </li>
                    <li class="insert-li"> <input placeholder="Shop email" name="email" type="text" class="insert-input"> </li>
                </ul>
                <div class="insert-btn-div">
                    <button class="insert-submit" type="submit">Submit</button>
                </div>

            </form>
        </div>

        <div class="insertContainer" id="product-div">
            <h2 class="insert-name">insert new Product</h2>

            <form action="/addProduct" method="post">

                <ul class="insertlist">
                    <li class="insert-li"><input placeholder="Product id" name="id" type="text" class="insert-input"> </li>
                    <li class="insert-li"><input placeholder="Product name" name="name" type="text" class="insert-input"> </li>
                    <li class="insert-li"><input placeholder="Product price" name="price" type="text" class="insert-input"> </li>
                    <li class="insert-li"><input placeholder="Product stocks" name="type" type="text" class="insert-input"> </li>
                    <li class="insert-li"><input placeholder="Product type" name="rev_id" type="text" class="insert-input"> </li>
                    <li class="insert-li"><input placeholder="Review id" name="brand" type="text" class="insert-input"> </li>
                </ul>
                <div class="insert-btn-div">
                    <button class="insert-submit" type="submit">Submit</button>
                </div>

            </form>
        </div>


    </div>

